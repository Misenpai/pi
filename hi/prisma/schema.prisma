generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model PI {
  username String @id
  password String

  @@map("pI")
}

model PIProjectRelation {
  id          Int    @id @default(autoincrement())
  username    String
  projectCode String

  @@unique([username, projectCode])
  @@map("pIProjectRelation")
}
